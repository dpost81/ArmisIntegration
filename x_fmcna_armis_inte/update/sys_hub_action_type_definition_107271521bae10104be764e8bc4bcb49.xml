  return ans; 
} 

(function execute(inputs, outputs) {
 var debug = []
 var q = new GlideRecord('cmdb_ci_computer')
 q.addQuery('name','TESTDEPLOY02')
 q.query()
 while(q.next())
 {
   debug.push("Queried: " + q.getValue('name')) 
   q.comments = generateID()
   q.update()
 }
 
 outputs.debug = debug.join('\n')
})(inputs, outputs);
</value>
